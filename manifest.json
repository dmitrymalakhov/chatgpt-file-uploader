{
  "name": "chatgpt-file-uploader",
  "version": "1.0",
  "description": "ChatGPT File Uploader",
  "manifest_version": 3,
  "permissions": ["activeTab", "downloads"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
